        <class_source>import com.github.xiaoyao9184.AvroPayloadConverter;


private AvroPayloadConverter converter;


public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
  this.converter = new AvroPayloadConverter("mock://me:8081");

  return parent.initImpl(stepMetaInterface, stepDataInterface);
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }

  r = createOutputRow(r, data.outputRowMeta.size());

  byte[] keyDataAvro = get(Fields.In, "kafka_key").getBinary(r);
  String keySchemaAvro = get(Fields.In, "key_schema_avro").getString(r);
  String keyDataConnect = this.converter.toConnectJson(true, keyDataAvro, keySchemaAvro);
  get(Fields.Out, "key_payload_json").setValue(r, keyDataConnect);

  byte[] messageDataAvro = get(Fields.In, "kafka_message").getBinary(r);
  String messageSchemaAvro = get(Fields.In, "message_schema_avro").getString(r);
  String messageDataConnect = this.converter.toConnectJson(false, messageDataAvro, messageSchemaAvro);
  get(Fields.Out, "message_payload_json").setValue(r, messageDataConnect);


  putRow(data.outputRowMeta, r);

  return true;
}
