        <class_source>import java.util.HashMap;
import java.util.Map;
import io.confluent.kafka.serializers.KafkaAvroDeserializer;
import io.confluent.kafka.schemaregistry.avro.AvroSchemaUtils;


public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }

  r = createOutputRow(r, data.outputRowMeta.size());

  byte[] keyDataAvro = get(Fields.In, "kafka_key").getBinary(r);
  String keyDataConnect = toConnectData(keyDataAvro,true);
  get(Fields.Out, "key_payload_json").setValue(r, keyDataConnect);

  byte[] messageDataAvro = get(Fields.In, "kafka_message").getBinary(r);
  String messageDataConnect = toConnectData(messageDataAvro,false);
  get(Fields.Out, "message_payload_json").setValue(r, messageDataConnect);


  putRow(data.outputRowMeta, r);

  return true;
}


private String schemaRegistryUrl = getVariable("Config.CDC.Kafka.Schema.Registry", "http://me:58081");
private Map&lt;String, Object> config = null;
private KafkaAvroDeserializer deserializerKey = null;
private KafkaAvroDeserializer deserializerMsg = null;

public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
  config = new HashMap&lt;String, Object>();
  config.put("schema.registry.url",schemaRegistryUrl);

  deserializerKey = new KafkaAvroDeserializer();
  deserializerKey.configure(config,true);

  deserializerMsg = new KafkaAvroDeserializer();
  deserializerMsg.configure(config,false);

  return parent.initImpl(stepMetaInterface, stepDataInterface);
}


public String toConnectData(byte[] payload, boolean isKey){
  if (payload == null) {
    return null;
  }
  try {
    KafkaAvroDeserializer deserializer = isKey ? deserializerKey : deserializerMsg;
    Object obj = deserializer.deserialize("",payload);
    byte[] bytes = AvroSchemaUtils.toJson(obj);
    String json = new String(bytes);
    return json;
  } catch (Exception e) {
    throw new RuntimeException(e);
  }
}

