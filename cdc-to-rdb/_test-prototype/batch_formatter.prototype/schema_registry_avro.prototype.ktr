public boolean init(StepMetaInterface stepMetaInterface, StepDataInterface stepDataInterface) {
  this.converter = new AvroSchemaConverter(10000);

  return parent.initImpl(stepMetaInterface, stepDataInterface);
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {

  Object[] r = getRow();

  if (r == null) {
    setOutputDone();
    return false;
  }

  r = createOutputRow(r, data.outputRowMeta.size());

  String keySchemaAvro = get(Fields.In, "key_schema_json").getString(r);
  String keySchemaConnect = this.converter.toConnectJson(true, keySchemaAvro);
  get(Fields.Out, "key_schema_avro").setValue(r, keySchemaAvro);
  get(Fields.In, "key_schema_json").setValue(r, keySchemaConnect);

  String messageSchemaAvro = get(Fields.In, "message_schema_json").getString(r);
  String messageSchemaConnect = this.converter.toConnectJson(false, messageSchemaAvro);
  get(Fields.Out, "message_schema_avro").setValue(r, messageSchemaAvro);
  get(Fields.In, "message_schema_json").setValue(r, messageSchemaConnect);


  putRow(data.outputRowMeta, r);

  return true;
}
