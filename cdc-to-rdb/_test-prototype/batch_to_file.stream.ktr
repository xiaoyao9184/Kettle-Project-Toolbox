        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import java.util.regex.Pattern;

import org.pentaho.di.trans.Trans;

private Trans trans;
private String[] variableList = {};
private int idx = 0;
private Pattern p = null;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{

	if (first){
		first = false;

		trans = getTrans();
		variableList = trans.listVariables();
		idx = 0;

		String regexString = getParameter("regex");
		p = Pattern.compile(regexString);
	}

	if (idx >= variableList.length) {
		setOutputDone();
		return false;
	}

	final String variableName = variableList[idx];

	if (p.matcher(variableName).matches()){

		// Substitute variables in provided property value.
		final String variableValue = getVariable(variableName);
		final String value = trans.environmentSubstitute(variableValue);

		// Create a row and set field values
		Object[] r = RowDataUtil.allocateRowData(data.outputRowMeta.size());
		get(Fields.Out, "key").setValue(r, variableName);
		get(Fields.Out, "value").setValue(r, value);

		// Send the row on to the next step.
		putRow(data.outputRowMeta, r);
	}

	idx++;

	return true;
}</class_source>
