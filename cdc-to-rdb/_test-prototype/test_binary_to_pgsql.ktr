	        var content = "";
            content = content + 'source:' + value + "\n";
            content = content + 'target type:' + typeof bytes + ' len:'+ bytes.length + "\n";
            content = content + 'target content index[0]:' + bytes[0] + ';content index[1]:' + bytes[1] + "\n"; 
			//Alert(content);
			value = bytes;
		} else {
			value = null;
		}
