//debug
var info = "";

var row = createRowCopy(getOutputRowMeta().size());
var fieldNames = getOutputRowMeta().getFieldNames();
for (index = 0; index &lt; fieldNames.length; index++) {
	var inMeta = getInputRowMeta().getValueMeta(index);
