<config>
    <project name="kpt-cdc-to-rdb">
        <cfg key="Name">PDI project:kpt-cdc-to-rdb(use xml in rep dir)</cfg>
        <cfg key="Repository">kpt-cdc-to-rdb</cfg>
        <cfg key="Version">1.0</cfg>
