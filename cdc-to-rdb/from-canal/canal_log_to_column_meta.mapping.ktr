    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>trans_Status = SKIP_TRANSFORMATION;

var index_column = getOutputRowMeta().indexOfValue("column");
var index_sql_type_number = getOutputRowMeta().indexOfValue("sql_type_number");
var index_mysql_type = getOutputRowMeta().indexOfValue("mysql_type");
var index_mysql_scale = getOutputRowMeta().indexOfValue("mysql_scale");
var index_mysql_precision = getOutputRowMeta().indexOfValue("mysql_precision");

var message_index = getInputRowMeta().indexOfValue("kafka_message");
var message_col = row[message_index];
var message_obj = JSON.parse(message_col);
var sqlType = Object.keys(message_obj.sqlType);

for (index = 0; index &lt; sqlType.length; index++) {
  var key = sqlType[index];

  var sql_type_number = message_obj.sqlType[key];
  var mysql_type = message_obj.mysqlType == null ? '' : message_obj.mysqlType[key];
  
  var row = createRowCopy(getOutputRowMeta().size());
  row[index_column] = key;
  row[index_sql_type_number] = Packages.java.lang.Long.valueOf(sql_type_number);
  row[index_mysql_type] = mysql_type;
  // only mysql type decimal
  if (3 == sql_type_number) { 
    // parse like 'decimal(10,1)' to '10,1'

    var index_start = indexOf(mysql_type, "(");
    var index_end = indexOf(mysql_type, ")");
    var index_split = indexOf(mysql_type, ",");

    var precision = substr(mysql_type, index_start + 1, index_split - index_start - 1);
    var scale = substr(mysql_type, index_split + 1, index_end - index_split - 1);    

    row[index_mysql_precision] = Packages.java.lang.Long.valueOf(precision);
    row[index_mysql_scale] = Packages.java.lang.Long.valueOf(scale);
  }
  putRow(row);
}
</jsScript_script>
      </jsScript>
