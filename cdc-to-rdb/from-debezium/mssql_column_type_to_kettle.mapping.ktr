        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>trans_Status = SKIP_TRANSFORMATION;

var index_field = getOutputRowMeta().indexOfValue("schema_field");
var index_type = getOutputRowMeta().indexOfValue("schema_type");
var index_name = getOutputRowMeta().indexOfValue("schema_name");

var message_index = getInputRowMeta().indexOfValue("message");
var message_col = row[message_index];
var message_object = JSON.parse(message_col);

var after_schema = null;
var fields = message_object.schema.fields;
for (index = 0; index &lt; fields.length; index++) {
  var field = fields[index];
  if(field.field == 'after'){
    after_schema = field;
    break;
  }else{
    continue;
  }
}

for (index = 0; index &lt; after_schema.fields.length; index++) {
  var schema_field = after_schema.fields[index];

  var field = schema_field.field;
  var type = schema_field.type;
  var name = schema_field.name;

  if (typeof name === 'undefined') {
      name = null;
  }

  var row = createRowCopy(getOutputRowMeta().size());
  row[index_field] = field;
  row[index_type] = type;
  row[index_name] = name;
  putRow(row);
}</jsScript_script>
