	if(inMeta.getTypeDesc() == 'String' &amp;&amp; outMate.getTypeDesc() == 'BigNumber'){
		var value = row[index];
		outMate.setConversionMask(null);
		if(value != null &amp;&amp; value.length() != 0) {
			var format = inMeta.getConversionMask().split(',');
